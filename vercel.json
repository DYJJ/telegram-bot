{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "api/webhook.py"
    },
    {
      "src": "/api/debug",
      "dest": "api/debug.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
} 